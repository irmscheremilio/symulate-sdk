#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/emilioirmscher/prog_private/mockend/node_modules/.pnpm/vitest@1.6.1_@types+node@20.19.23_less@4.2.0_sass@1.77.6_terser@5.44.0/node_modules/vitest/node_modules:/Users/emilioirmscher/prog_private/mockend/node_modules/.pnpm/vitest@1.6.1_@types+node@20.19.23_less@4.2.0_sass@1.77.6_terser@5.44.0/node_modules:/Users/emilioirmscher/prog_private/mockend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/emilioirmscher/prog_private/mockend/node_modules/.pnpm/vitest@1.6.1_@types+node@20.19.23_less@4.2.0_sass@1.77.6_terser@5.44.0/node_modules/vitest/node_modules:/Users/emilioirmscher/prog_private/mockend/node_modules/.pnpm/vitest@1.6.1_@types+node@20.19.23_less@4.2.0_sass@1.77.6_terser@5.44.0/node_modules:/Users/emilioirmscher/prog_private/mockend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
